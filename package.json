{
  "name": "simple-lambda-api-router",
  "version": "1.0.3",
  "files": [
    "dist/*"
  ],
  "main": "dist/index.js",
  "scripts": {
    "compile": "npm run test && npm run compile:clear && npm run compile:ts",
    "compile:ts": "tsc && npm run test:modules",
    "compile:bundle": "node build.js",
    "compile:clear": "rm -rf dist",
    "test": "jest",
    "test:modules": "npm run test:commonjs && npm run test:tsmodule",
    "test:commonjs": "npx jest --testMatch \"$(pwd)/commonjs.test.js\"",
    "test:tsmodule": "npx jest --testMatch \"$(pwd)/tsmodule.test.ts\"",
    "publish:npm:patch": "npm run test:modules && npm version patch && npm publish"
  },
  "license": "ISC",
  "dependencies": {
    "http-errors": "^2.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/aws-lambda": "^8.10.92",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild": "^0.17.14",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
